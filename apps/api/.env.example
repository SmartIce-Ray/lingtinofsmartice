# Backend Environment Variables Template - v1.1
# Copy this file to .env and fill in your values

# Server
PORT=3001

# Supabase
SUPABASE_URL=https://your-project.supabase.co
SUPABASE_SERVICE_KEY=your-service-role-key

# 讯飞 STT API
XUNFEI_API_KEY=your-xunfei-api-key
XUNFEI_API_SECRET=your-xunfei-api-secret
XUNFEI_APP_ID=your-xunfei-app-id

# OpenRouter API (for AI processing and chat)
OPENROUTER_API_KEY=your-openrouter-api-key

# DashScope API (阿里 Paraformer-v2 STT, optional - falls back to 讯飞 if not set)
DASHSCOPE_API_KEY=your-dashscope-api-key

# Frontend URL (for CORS)
FRONTEND_URL=https://your-frontend-domain.com
